    "animate.css": "^4.1.1",
    "axios": "^1.7.7",
    "element-plus": "^2.8.2",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.2",
    "pinia-plugin-persistedstate": "^4.0.0",
    "sass": "^1.78.0",
    "universal-cookie": "^7.2.0",
    "vant": "^4.9.5",
    "vite-plugin-eslint": "^1.8.1",
    "vue": "3.5",
    "vue-router": "^4.4.3"
  },
  "devDependencies": {
